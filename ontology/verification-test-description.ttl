                                                                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                           owl:onClass <https://w3id.org/def/vtc#TestCaseResult>
                                                                         ] ,
                                                                         [ rdf:type owl:Restriction ;
                                                                           owl:onProperty <https://w3id.org/def/vtc#hasExecution> ;
